{
  "name": "pdfer",
  "version": "1.0.0",
  "description": "simple cli to create pdf from folder contains images",
  "type": "module",
  "keywords": ["cli", "pdf", "images"],
  "bin": {
    "pdfer": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "node  ./dist/index.js",
    "build": "tsc",
    "test": "jest",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "dev": "nodemon  dist/index",
    "ts": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write"
  },
  "author": "nagy-nabil",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/pdfkit": "^0.12.8",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "pdfkit": "^0.13.0"
  }
}
